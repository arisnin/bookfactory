<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bf.mapper.MainMapper">
	<insert id="register" parameterType="com.bf.main.dto.RegisterDto">
		insert into member values(member_num_seq.nextval,#{id},#{password},#{email},#{gender},sysdate,#{birthday},#{marketing_check},#{ip},sysdate,#{phone},#{name})
	</insert>
		
	
	<select id="noticeMainCount" resultType="int">
			select count(*) from notice
	</select>
	
	<!-- 
	
	<select id="noticeMain" parameterType="java.util.Map" resultMap="com.bf.main.dto.NoticeDto">
		select * from notice where num=#{num} desc;
	</select>
	 -->
	
</mapper>	